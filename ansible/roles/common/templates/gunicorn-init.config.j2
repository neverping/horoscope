bind = "0.0.0.0:8009"

user = "{{project_user}}"
group = "vagrant"

workers = 2
worker_class = "sync"
backlog = 64
max_requests = 300
timeout = 120

preload_app = True
daemon = True

pidfile = "{{ gunicorn_pid_file }}"

logconfig = "{{ gunicorn_directory }}/gunicorn-logging.config"
loglevel = "ERROR"

debug = False
timeout = 120
